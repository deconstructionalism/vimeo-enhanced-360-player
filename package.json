{
  "name": "vimeo-no-code-video-player",
  "version": "1.0.0",
  "description": "Wrapper around Vimeo's player.js that allows configuration of all vimeo player settings using HTML",
  "main": "src/index.js",
  "scripts": {
    "build": "BUILD=1 npx rollup --config ./rollup.config.js",
    "format": "npx prettier --write src/**/*.ts",
    "format:ci": "npx prettier --check src/**/*.ts",
    "lint:ci": "npx tslint --config ./tslint.json --project ./tsconfig.json",
    "lint": "npx tslint --config ./tslint.json --project ./tsconfig.json --fix",
    "start": "npx rollup --watch --config ./rollup.config.js",
    "test": "npx jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deconstructionalism/vimeo-no-code-video-player.git"
  },
  "keywords": [
    "Vimeo",
    "video",
    "VR",
    "360"
  ],
  "author": "Arjun Ray",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/deconstructionalism/vimeo-no-code-video-player/issues"
  },
  "homepage": "https://github.com/deconstructionalism/vimeo-no-code-video-player#readme",
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.3",
    "@types/jest": "^29.5.3",
    "jest-environment-jsdom": "^29.6.2",
    "prettier": "2.8.8",
    "rollup": "^3.25.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-serve": "^2.0.2",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript2": "^0.34.1",
    "ts-jest": "^29.1.1",
    "tslib": "^2.5.3",
    "tslint": "^6.1.3",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@types/vimeo__player": "^2.18.0",
    "@vimeo/player": "^2.20.1"
  }
}
